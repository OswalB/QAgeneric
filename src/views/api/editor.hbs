<script src="https://cdnjs.cloudflare.com/ajax/libs/xlsx/0.16.2/xlsx.full.min.js"></script>
<!-- Modal editor -->
<div class="modal fade" id="modalEditor" tabindex="-1" aria-labelledby="modalEditorLabel" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modal-title"></h5>
        
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <table class="table table-success table-striped">
          <thead>
            <tr>
              <th scope="col">Campo</th>
              <th scope="col">Valor</th>
            </tr>
          </thead>
          <tbody id="bodyTable">
            
          </tbody>
          
        </table>
      </div>
      <div class="modal-footer">
        <a id="btn_reset" class="btn btn-secondary" href="#"><i class="fa fa-wrench fa-lg" aria-hidden="true"></i> Reset pass</a>
        <a id="btn_borrar" class="btn btn-danger" href="#"><i class="fa fa-times-circle fa-lg" aria-hidden="true"></i> Delete</a>
        <a id="btn_guardar" class="btn btn-success" href="#"><i class="fa fa-floppy-o fa-lg" aria-hidden="true"></i> Guardar</a>
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal"><i class="fa fa-sign-out fa-lg" aria-hidden="true"></i> Salir</button>
      </div>
    </div>
  </div>
</div>
<!-- modal select xls  file-->
<div  class="modal fade" id="modalFilexls" tabindex="-1"  aria-hidden="true">
  <div class="modal-dialog"> 
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="modalLabelf"></h5>
      </div>
      <div class="modal-body">
        <div class="mb-3">
          <input class="form-control" type="file" accept=".xls,.xlsx" id="inputFile">
        </div>
      </div>
      <div class="modal-footer">
        <button id = "btnSendxls" type="button" class="btn btn-primary" data-bs-dismiss="modal" style="display : none;"><i class="fa fa-upload fa-lg" aria-hidden="true"></i> Enviar</button>
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal"><i class="fa fa-sign-out fa-lg" aria-hidden="true"></i> Salir</button>
      </div>
    </div>
  </div>
</div>

<table class="table table-success table-striped table-hover">
  <thead>
    <tr id="encabezado">
      
    </tr>
  </thead>
  <tbody id="bodyContainer" >
    
  </tbody>
</table>

